        android:gravity="center"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/bracket_player_2_mapwins"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/bracket_player_2_flag"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/bracket_player_2_race"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/bracket_player_2_name"
        android:layout_weight="1"
